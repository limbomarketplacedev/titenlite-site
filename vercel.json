{
  "version": 3,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/save-email.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
